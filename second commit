#include <bits/stdc++.h>

using namespace std; 

int main() {
    
    int T;
    cin>>T;

    while(T--){

        int a;
        cin>>a;

        string s= to_string(a);

        int n=s.size();

        if(s[0]=='1' && s[1]=='0' && s[2]!='0'){
            if(n==3 && s[2]=='1'){
                cout<<"NO\n";
                continue;
            }
            cout<<"YES\n";
        }
        else{
            cout<<"NO\n";
        }
    }


}
